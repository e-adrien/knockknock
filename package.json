{
  "name": "knockknock",
  "version": "1.0.0",
  "description": "Wake up devices on the LAN",
  "author": "Adrien ERAUD",
  "contributors": [
    "Adrien ERAUD <e-adrien@users.noreply.github.com>"
  ],
  "engines": {
    "node": "20.x"
  },
  "license": "MIT",
  "browserslist": "> 1% and last 2 version, not dead",
  "scripts": {
    "freshlock": "rm -rf node_modules/ && rm package-lock.json && npm install",
    "eslint-check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "prettier-check": "prettier --check \"**/*.{mjs,cjs,ts,js,json,yml,scss,pug}\"",
    "prettier-fix": "prettier --write \"**/*.{mjs,cjs,ts,js,json,yml,scss,pug}\"",
    "tsc-check": "tsc --noEmit --project server/tsconfig.json ",
    "qc": "npm run eslint-check && npm run stylelint-check && npm run prettier-check && npm run tsc-check",
    "test": "NODE_OPTIONS='--loader=ts-node/esm' TS_NODE_PROJECT='server/tsconfig.json' mocha --config server/.mocharc.json",
    "build-client": "webpack --config public/webpack.config.cjs",
    "build-server": "tsc --project server/tsconfig.json && node server/assets.cjs",
    "build": "npm run build-client && npm run build-server",
    "deploy-commands": "node dist/deploy-commands.js",
    "create-hue-api-key": "node dist/create-hue-api-key.js",
    "list-hue-devices": "node dist/list-hue-devices.js",
    "start": "node dist/index.js"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/connect-ensure-login": "^0.1.9",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.0",
    "@types/method-override": "^0.0.35",
    "@types/nconf": "^0.10.6",
    "@types/node": "^20.12.7",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pug": "^2.0.10",
    "autoprefixer": "^10.4.19",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "connect-ensure-login": "^0.1.1",
    "css-loader": "^7.1.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "cssnano": "^6.1.2",
    "discord.js": "^14.14.1",
    "execa": "^8.0.1",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "memorystore": "^1.6.7",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "nconf": "^0.12.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "pug": "^3.0.2",
    "sass": "^1.74.1",
    "sass-loader": "^14.1.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "undici": "^6.12.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.0.0",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "ts-node": "^10.9.2"
  }
}
