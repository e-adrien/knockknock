{
  "name": "knockknock",
  "version": "1.0.0",
  "description": "Wake up devices on the LAN",
  "author": "Adrien ERAUD",
  "contributors": [
    "Adrien ERAUD <e-adrien@users.noreply.github.com>"
  ],
  "engines": {
    "node": "18.x || 20.x"
  },
  "license": "MIT",
  "browserslist": "> 1% and last 2 version, not dead",
  "scripts": {
    "freshlock": "rm -rf node_modules/ && rm package-lock.json && npm install",
    "eslint-check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "prettier-check": "prettier --check \"**/*.{mjs,cjs,ts,js,json,yml,scss,pug}\"",
    "prettier-fix": "prettier --write \"**/*.{mjs,cjs,ts,js,json,yml,scss,pug}\"",
    "tsc-check": "tsc --noEmit --project server/tsconfig.json ",
    "qc": "npm run eslint-check && npm run stylelint-check && npm run prettier-check && npm run tsc-check",
    "build-client": "webpack --config public/webpack.config.cjs",
    "build-server": "tsc --project server/tsconfig.json && node server/assets.cjs",
    "build": "npm run build-client && npm run build-server",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tsconfig/next": "^2.0.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/bootstrap": "^5.2.7",
    "@types/connect-ensure-login": "^0.1.7",
    "@types/express": "^4.17.18",
    "@types/express-session": "^1.17.8",
    "@types/fs-extra": "^11.0.2",
    "@types/ip": "^1.1.1",
    "@types/lodash": "^4.14.199",
    "@types/method-override": "^0.0.33",
    "@types/nconf": "^0.10.4",
    "@types/node": "^20.7.1",
    "@types/passport": "^1.0.13",
    "@types/passport-local": "^1.0.36",
    "@types/pug": "^2.0.7",
    "autoprefixer": "^10.4.16",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "connect-ensure-login": "^0.1.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "get-ip-range": "^4.0.1",
    "ip": "^1.1.8",
    "lodash": "^4.17.21",
    "memorystore": "^1.6.7",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nconf": "^0.12.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "pug": "^3.0.2",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@prettier/plugin-pug": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "ts-node": "^10.9.1"
  }
}
