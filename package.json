{
  "name": "knockknock",
  "version": "2.0.3",
  "description": "Wake up devices on the LAN",
  "author": "Adrien ERAUD",
  "contributors": [
    "Adrien ERAUD <e-adrien@users.noreply.github.com>"
  ],
  "engines": {
    "node": "22.x || 24.x"
  },
  "license": "MIT",
  "browserslist": "> 1% and last 2 version, not dead",
  "scripts": {
    "freshlock": "rm -rf node_modules/ && rm .yarn/install-state.gz && rm yarn.lock && yarn",
    "eslint-check": "eslint",
    "eslint-fix": "eslint --fix",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "tsc-check": "tsc --noEmit --project server/tsconfig.json ",
    "qc": "npm run eslint-check && npm run stylelint-check && npm run prettier-check && npm run tsc-check",
    "test": "NODE_OPTIONS='--loader=ts-node/esm' TS_NODE_PROJECT='server/tsconfig.json' mocha --config server/.mocharc.json",
    "build-client": "webpack --config public/webpack.config.cjs",
    "build-server": "tsc --project server/tsconfig.json && node server/assets.cjs",
    "build": "npm run build-client && npm run build-server",
    "deploy-commands": "node dist/deploy-commands.js",
    "create-hue-api-key": "node dist/create-hue-api-key.js",
    "list-hue-devices": "node dist/list-hue-devices.js",
    "get-ring-refresh-token": "npx -p ring-client-api ring-auth-cli",
    "list-ring-devices": "node dist/list-ring-devices.js",
    "start": "node dist/index.js"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tsconfig/next": "^2.0.5",
    "@tsconfig/node22": "^22.0.5",
    "@types/bootstrap": "^5.2.10",
    "@types/connect-ensure-login": "^0.1.9",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.24",
    "@types/method-override": "^3.0.0",
    "@types/nconf": "^0.10.7",
    "@types/node": "^24.10.13",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/pug": "^2.0.10",
    "autoprefixer": "^10.4.27",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "connect-ensure-login": "^0.1.1",
    "css-loader": "^7.1.4",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "cssnano": "^7.1.2",
    "discord.js": "^14.25.1",
    "execa": "^9.6.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.19.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.3.3",
    "lodash": "^4.17.23",
    "memorystore": "^1.6.7",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "^2.10.0",
    "nconf": "^0.13.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.1",
    "pug": "^3.0.3",
    "ring-client-api": "14.3.0",
    "sass": "^1.97.3",
    "sass-loader": "^16.0.7",
    "suncalc-ts": "^3.0.2",
    "terser-webpack-plugin": "^5.3.16",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "undici": "^7.22.0",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@prettier/plugin-pug": "3.4.2",
    "@types/mocha": "10.0.10",
    "eslint": "10.0.2",
    "eslint-config-prettier": "10.1.8",
    "globals": "17.3.0",
    "mocha": "11.7.5",
    "nodemon": "3.1.14",
    "prettier": "3.8.1",
    "stylelint": "17.4.0",
    "stylelint-config-standard-scss": "17.0.0",
    "ts-node": "10.9.2",
    "typescript-eslint": "8.56.1"
  },
  "dependenciesMeta": {
    "@parcel/watcher": {
      "built": true
    },
    "ffmpeg-for-homebridge": {
      "built": true
    },
    "protobufjs": {
      "built": true
    }
  },
  "packageManager": "yarn@4.12.0"
}
