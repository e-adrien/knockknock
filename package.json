{
  "name": "knockknock",
  "version": "1.2.1",
  "description": "Wake up devices on the LAN",
  "author": "Adrien ERAUD",
  "contributors": [
    "Adrien ERAUD <e-adrien@users.noreply.github.com>"
  ],
  "engines": {
    "node": "20.x || 22.x"
  },
  "license": "MIT",
  "browserslist": "> 1% and last 2 version, not dead",
  "scripts": {
    "freshlock": "rm -rf node_modules/ && rm package-lock.json && npm install",
    "eslint-check": "eslint",
    "eslint-fix": "eslint --fix",
    "stylelint-check": "stylelint **/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "tsc-check": "tsc --noEmit --project server/tsconfig.json ",
    "qc": "npm run eslint-check && npm run stylelint-check && npm run prettier-check && npm run tsc-check",
    "test": "NODE_OPTIONS='--loader=ts-node/esm' TS_NODE_PROJECT='server/tsconfig.json' mocha --config server/.mocharc.json",
    "build-client": "webpack --config public/webpack.config.cjs",
    "build-server": "tsc --project server/tsconfig.json && node server/assets.cjs",
    "build": "npm run build-client && npm run build-server",
    "deploy-commands": "node dist/deploy-commands.js",
    "create-hue-api-key": "node dist/create-hue-api-key.js",
    "list-hue-devices": "node dist/list-hue-devices.js",
    "get-ring-refresh-token": "npx -p ring-client-api ring-auth-cli",
    "list-ring-devices": "node dist/list-ring-devices.js",
    "start": "node dist/index.js"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/connect-ensure-login": "^0.1.9",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.7",
    "@types/method-override": "^0.0.35",
    "@types/nconf": "^0.10.7",
    "@types/node": "^20.16.1",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/pug": "^2.0.10",
    "autoprefixer": "^10.4.20",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "connect-ensure-login": "^0.1.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "cssnano": "^7.0.5",
    "discord.js": "^14.15.3",
    "execa": "^9.3.1",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "memorystore": "^1.6.7",
    "method-override": "^3.0.0",
    "mini-css-extract-plugin": "^2.9.1",
    "nconf": "^0.12.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "picocolors": "^1.0.1",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "pug": "^3.0.3",
    "ring-client-api": "^13.1.0",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.1",
    "suncalc-ts": "^2.0.1",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "undici": "^6.19.8",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@prettier/plugin-pug": "^3.0.0",
    "@types/mocha": "^10.0.7",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "stylelint": "^16.8.2",
    "stylelint-config-sass-guidelines": "^12.0.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.2.0"
  }
}